{
    "installCommand":"npm install --legacy-peer-deps",
    "version":2,
    "builds":[
        {
            "src":"index.js",
            "use":"@vercel/node"
        },
        {
            "src":"src/**/*",
            "user":"@vercel/static"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"/"
        }
    ]
}